{
  "expo": {
    "name": "Deeper",
    "slug": "deeper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/placeholder/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "deeper",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/placeholder/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0D0A12"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.deeper"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/placeholder/logo.png",
        "backgroundColor": "#0D0A12"
      },
      "package": "com.yourname.deeper",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/placeholder/logo.png"
    }
  }
}
